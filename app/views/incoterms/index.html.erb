<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-2xl">Incoterms</h1>
  </div>

  <hr class="mt-3 mb-6" />

  <div id="incoterms" class="xl:w-2/3 mx-auto">
    <%= render @incoterms %>
  </div>
</div>
